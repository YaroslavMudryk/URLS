{
    "profiles": {
        "URLS (http)": {
            "commandName": "Project",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "launchUrl": "http://localhost:5029/openapi/v1/openapi.json",
            "applicationUrl": "http://localhost:5029"
        },
        "URLS (https)": {
            "commandName": "Project",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "launchUrl": "https://localhost:7133/openapi/v1/openapi.json;",
            "applicationUrl": "https://localhost:7133;http://localhost:5029"
        },
        "URLS (Dockerfile)": {
            "commandName": "Docker",
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
            "environmentVariables": {
                "ASPNETCORE_HTTPS_PORTS": "8081",
                "ASPNETCORE_HTTP_PORTS": "8080"
            },
            "publishAllPorts": true,
            "useSSL": true
        }
    },
    "$schema": "https://json.schemastore.org/launchsettings.json"
}